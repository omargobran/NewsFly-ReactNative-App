{
  "expo": {
    "name": "News Fly",
    "icon": "./assets/icon.png",
    "slug": "news-fly",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "ios":{
      "bundleIdentifier": "expo.ios.package",
      "supportsTablet": true,
      "icon":"./assets/icon.png"
    },
    "android":{
      "package":"expo.android.package",
      "icon":"./assets/icon.png"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}